pipeline {
  agent any
  stages {
    stage('Pre-Build') {
      steps {
        echo 'Pre-build'
      }
    }

    stage('Evaluate Build') {
      steps {
        echo 'Build evaluated'
      }
    }

    stage('Build') {
      steps {
        echo 'Build now'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Deploy!'
      }
    }

  }
}